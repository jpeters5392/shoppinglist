<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="shoppinglist.App">
	<Application.Resources>
        <ResourceDictionary>
            <Color x:Key="Primary">#00838f</Color>
            <Color x:Key="PrimaryDark">#005662</Color>
            <Color x:Key="Accent">#f9a825</Color>
            <Color x:Key="PrimaryText">#ffffff</Color>
            <Color x:Key="AccentText">#000000</Color>

            <Style x:Key="MealLabel" TargetType="Label">
                <Setter Property="Margin" Value="5,0,5,0" />
            </Style>
        </ResourceDictionary>
	</Application.Resources>
</Application>
