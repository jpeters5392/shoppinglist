<?xml version="1.0" encoding="UTF-8"?>
<reactive:ReactiveViewCell 
x:TypeArguments="local:MealItemGroupViewModel"
xmlns="http://xamarin.com/schemas/2014/forms" 
xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
x:Class="shoppinglist.Cells.MealItemGroup"
xmlns:local="clr-namespace:shoppinglist.ViewModels;assembly=shoppinglist"
xmlns:reactive="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms">
    <ContentView BackgroundColor="{StaticResource PrimaryDark}" Padding="5" HorizontalOptions="FillAndExpand">
        <Label x:Name="GroupName" TextColor="{StaticResource PrimaryText}" FontSize="Large" VerticalTextAlignment="Center" HorizontalTextAlignment="Center" />
    </ContentView>
</reactive:ReactiveViewCell>